{
  "name": "vue",
  "version": "0.0.0",
  "description": "powered by vue",
  "type": "module",
  "scripts": {
    "dev": "vite"
  },
  "author": "haominzh",
  "license": "MIT",
  "dependencies": {
    "@vueuse/core": "^10.6.1",
    "ky": "^1.1.3",
    "pinia": "^2.1.7",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "^0.17.5",
    "@testing-library/vue": "^8.0.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/test-utils": "^2.4.2",
    "eslint-plugin-vue": "^9.18.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "@stylistic/ts",
      "unicorn",
      "vue",
      "testing-library"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:unicorn/recommended",
      "plugin:vue/vue3-essential",
      "plugin:testing-library/vue"
    ],
    "rules": {
      "unicorn/filename-case": [
        "error",
        {
          "cases": {
            "camelCase": true,
            "pascalCase": true
          },
          "ignore": [
            "HR",
            "CV"
          ]
        }
      ],
      "unicorn/prevent-abbreviations": [
        "error",
        {
          "allowList": {
            "props": true,
            "src": true
          }
        }
      ]
    }
  }
}
